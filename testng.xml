<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sample Test Suite" parallel="classes" thread-count="2">

<test name="FirstTest">
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="86.0"/>
    <classes>
          <class name="grumpy.GrumpyTest"/>
      </classes>
</test>
    <test name="SecondTest">
        <parameter name="browserName" value="firefox"/>
        <parameter name="browserVersion" value="82.0"/>
        <classes>
            <class name="grumpy.GrumpyTest"/>
        </classes>
    </test>
    <test name="ThirdTest">
        <parameter name="browserName" value="opera"/>
        <parameter name="browserVersion" value="72.0"/>
        <classes>
            <class name="grumpy.GrumpyTest"/>
        </classes>
    </test>
    <test name="MicrosoftEdgeTest">
        <parameter name="browserName" value="MicrosoftEdge"/>
        <parameter name="browserVersion" value="88.0"/>
        <classes>
            <class name="grumpy.GrumpyTest"/>
        </classes>
    </test>
    <test name="SafariTest">
        <parameter name="browserName" value="safari"/>
        <parameter name="browserVersion" value="13.0"/>
        <classes>
            <class name="grumpy.GrumpyTest"/>
        </classes>
    </test>
</suite>